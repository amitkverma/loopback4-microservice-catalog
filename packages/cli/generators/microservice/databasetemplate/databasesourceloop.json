{
  "defaultEnv": "master",
  "master": {
    "driver": "<DbKey>",
    "host": {
      "ENV": "<DbKey>_DB_HOST"
    },
    "port": {
      "ENV": "<DbKey>_DB_PORT"
    },
    "user": {
